{
    "name": "alablaster/foreman",
    "description": "Generator utility for Laravel Projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sean Alaback",
            "email": "sean.alaback@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.24",
        "phpunit/phpunit": "9.5.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Alablaster\\Foreman\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alablaster\\Foreman\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alablaster\\Foreman\\ForemanServiceProvider"
            ]
        }
    }
}
